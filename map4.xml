<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
        <Mission xmlns="http://ProjectMalmo.microsoft.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
          <About>
            <Summary>Fire Escape!</Summary>
          </About>
        <ServerSection>
          <ServerInitialConditions>
            <Time>
                <StartTime>1000</StartTime>
                <AllowPassageOfTime>false</AllowPassageOfTime>
            </Time>
            <Weather>clear</Weather>
          </ServerInitialConditions>
          <ServerHandlers>
              <FlatWorldGenerator generatorString="3;7,44*49,73,35:1,159:4,95:13,35:13,159:11,95:10,159:14,159:6,35:6,95:6;12;"/>
              <DrawingDecorator>
                <DrawSphere x="0" y="50" z="0" radius="30" type="air"/>
                <DrawLine x1="-2" y1="50" z1="0" x2="3" y2="50" z2="0" type="diamond_block"/>
                <DrawLine x1="-2" y1="50" z1="0" x2="-2" y2="50" z2="6" type="diamond_block"/>
                <DrawLine x1="-2" y1="50" z1="6" x2="3" y2="50" z2="6" type="diamond_block"/>
                <DrawBlock x="5" y="50" z="0" type="diamond_block"/>
                <DrawLine x1="2" y1="50" z1="1" x2="5" y2="50" z2="1" type="diamond_block"/>
                <DrawLine x1="0" y1="50" z1="2" x2="3" y2="50" z2="2" type="diamond_block"/>
                <DrawBlock x="0" y="50" z="3" type="diamond_block"/>
                <DrawLine x1="3" y1="50" z1="3" x2="5" y2="50" z2="3" type="diamond_block"/>
                <DrawLine x1="0" y1="50" z1="4" x2="1" y2="50" z2="4" type="diamond_block"/>
                <DrawBlock x="3" y="50" z="4" type="diamond_block"/>
                <DrawBlock x="5" y="50" z="4" type="diamond_block"/>
                <DrawLine x1="3" y1="50" z1="5" x2="5" y2="50" z2="5" type="diamond_block"/>
                <DrawBlock x="0" y="50" z="0" type="emerald_block"/>
                <DrawBlock x="5" y="50" z="4" type="redstone_block"/>
              </DrawingDecorator>
              <ServerQuitFromTimeUp timeLimitMs="50000"/>
              <ServerQuitWhenAnyAgentFinishes/>
            </ServerHandlers>
          </ServerSection>
          
          <AgentSection mode="Survival">
            <Name>FireEscapeBot</Name>
            <AgentStart>
                <Placement x="0.5" y="51" z="0.5" yaw="0"/>
            </AgentStart>
            <AgentHandlers>
              <ChatCommands/>
                <DiscreteMovementCommands/>
                <AgentQuitFromTouchingBlockType>
                    <Block type="redstone_block"/>
                </AgentQuitFromTouchingBlockType>
                <ObservationFromFullStats/>
                <ObservationFromGrid>
                  <Grid name="floorAll">
                    <min x="-10" y="-1" z="-10"/>
                    <max x="10" y="-1" z="10"/>
                  </Grid>
              </ObservationFromGrid>
            </AgentHandlers>
          </AgentSection>
        </Mission>